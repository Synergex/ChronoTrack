dblproto $VARIANT_FLAGS SRC:MailAddressCollection.dbc 
dbl $COMPILEFLAGS -TX -WD=919 $VARIANT_FLAGS  -o OBJ:MailAddressCollection.dbo SRC:MailAddressCollection.dbc 
