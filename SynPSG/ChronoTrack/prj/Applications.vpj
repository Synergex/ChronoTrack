<!DOCTYPE Project SYSTEM "http://www.slickedit.com/dtd/vse/10.0/vpj.dtd">
<Project
    Version="10.0"
    VendorName="SlickEdit"
    WorkingDir="../src/Applications/"
    VCSProject=""
    VCSLocalPath=""
    VCSAuxPath="">
    <Config
        Name="Release"
        OutputFile=""
        CompilerConfigName="Latest Version">
        <Menu>
            <Target
                Name="Compile"
                MenuCaption="&amp;Compile"
                CaptureOutputWith="ProcessBuffer"
                SaveOption="SaveCurrent"
                RunFromDir="%rw">
                <Exec CmdLine='"%(DBLDIR)\bin\dbl" %(SYN_DBG) -XT "%p%n%e"'/>
            </Target>
            <Target
                Name="Build"
                MenuCaption="&amp;Build"
                CaptureOutputWith="ProcessBuffer"
                SaveOption="SaveWorkspaceFiles"
                RunFromDir="%rw"
                ClearProcessBuffer="1">
                <Exec CmdLine="build -nologo"/>
            </Target>
            <Target
                Name="Rebuild"
                MenuCaption="&amp;Rebuild"
                RunFromDir="%rw"
                ShowOnMenu="HideIfNoCmdLine">
                <Exec/>
            </Target>
            <Target
                Name="Debug"
                MenuCaption="&amp;Debug"
                SaveOption="SaveCurrent"
                RunFromDir="%rw">
                <Exec CmdLine='"%(DBLDIR)\bin\dbr" -d "EXE:%n"'/>
            </Target>
            <Target
                Name="Execute"
                MenuCaption="E&amp;xecute"
                SaveOption="SaveCurrent"
                RunFromDir="%rw">
                <Exec CmdLine='"%(DBLDIR)\bin\dbr" "EXE:%n"'/>
            </Target>
            <Target
                Name="Compile Scripts"
                MenuCaption="Co&amp;mpile Scripts"
                CaptureOutputWith="ProcessBuffer"
                SaveOption="SaveAll"
                RunFromDir="%rw">
                <Exec
                    CmdLine="SynCompileScripts"
                    Type="Slick-C"/>
            </Target>
            <Target
                Name="Compile Scripts Setup"
                MenuCaption="Compile Scripts Set&amp;up..."
                CaptureOutputWith="ProcessBuffer"
                SaveOption="SaveAll"
                RunFromDir="%rw">
                <Exec
                    CmdLine="SynSetupCompileScripts"
                    Type="Slick-C"/>
            </Target>
            <Target
                Name="Generate Synergy Test Skeletons"
                MenuCaption="Generate Synergy Test S&amp;keletons..."
                CaptureOutputWith="ProcessBuffer"
                SaveOption="SaveCurrent"
                RunFromDir="%rw">
                <Exec
                    CmdLine="SynStartSkeletonGen"
                    Type="Slick-C"/>
            </Target>
            <Target
                Name="Generate Synergy Prototypes"
                MenuCaption="Generate Synergy Pro&amp;totypes..."
                CaptureOutputWith="ProcessBuffer"
                SaveOption="SaveAll"
                RunFromDir="%rw">
                <Exec
                    CmdLine="SynGeneratePrototypes"
                    Type="Slick-C"/>
            </Target>
            <Target
                Name="QuickBuild"
                MenuCaption="&amp;Quick Build"
                CaptureOutputWith="ProcessBuffer"
                ClearProcessBuffer="1"
                SaveOption="SaveWorkspaceFiles"
                RunFromDir="%rw">
                <Exec CmdLine="build -nologo"/>
            </Target>
            <Target
                Name="Build Test Programs"
                MenuCaption="&amp;Build Test Programs"
                CaptureOutputWith="ProcessBuffer"
                ClearProcessBuffer="1"
                SaveOption="SaveWorkspaceFiles">
                <Exec CmdLine="build test -nologo"/>
            </Target>
        </Menu>
        <PreBuildCommands>
            <Exec CmdLine="set MAKE_MAKEIT_FILE=YES"/>
            <Exec CmdLine="set MAKE_MAKEIT_UNIX_FILE=YES"/>
            <Exec CmdLine="build -nologo clean"/>
            <Exec CmdLine="build -nologo dbldeps"/>
        </PreBuildCommands>
        <PostBuildCommands>
            <Exec CmdLine="echo endlocal &gt;&gt; makeit.bat"/>
            <Exec CmdLine="set MAKE_MAKEIT_FILE=NO"/>
            <Exec CmdLine="set MAKE_MAKEIT_UNIX_FILES=NO"/>
        </PostBuildCommands>
    </Config>
    <Files>
        <Folder
            Name="Source Files"
            Filters="*.dbl;*.tpl">
            <F N="../src/Applications/AboutWindow.dbl"/>
            <F N="../src/Applications/ChangePassword.dbl"/>
            <F N="../src/Applications/ChronoTrack.dbl"/>
            <F N="../src/Applications/ChronoTrackMaint.dbl"/>
            <F N="../src/Applications/CountryMaintenance.dbl"/>
            <F N="../src/Applications/CustomerContactMaintenance.dbl"/>
            <F N="../src/Applications/CustomerLocationMaintenance.dbl"/>
            <F N="../src/Applications/CustomerMaintenance.dbl"/>
            <F N="../src/Applications/FeeTypeMaintenance.dbl"/>
            <F N="../src/Applications/LoginForm.dbl"/>
            <F N="../src/Applications/OptionsMaintenance.dbl"/>
            <F N="../src/Applications/ProjectCenter.dbl"/>
            <F N="../src/Applications/ProjectCreator.dbl"/>
            <F N="../src/Applications/ProjectCreator_tab.dbl"/>
            <F N="../src/Applications/ProjectManager.dbl"/>
            <F N="../src/Applications/ProjectManager_tab.dbl"/>
            <F N="../src/Applications/RequestCreator.dbl"/>
            <F N="../src/Applications/RunFeeTypeMaint.dbl"/>
            <F N="../src/Applications/RunStateMaint.dbl"/>
            <F N="../src/Applications/RunUserMaint.dbl"/>
            <F N="../src/Applications/StateMaintenance.dbl"/>
            <F N="../src/Applications/UserMaintenance.dbl"/>
            <F N="../src/Applications/UserMaintenance_tab.dbl"/>
            <F N="../src/Applications/xfpl_log.dbl"/>
        </Folder>
        <Folder
            Name="Header Files"
            Filters="*.def;*.dbh">
            <F N="../def/ChronoTrack.def"/>
            <F N="../def/ChronoTrackApp.def"/>
            <F N="../def/ChronoTrackMacros.def"/>
            <F N="../def/Version.def"/>
        </Folder>
        <Folder
            Name="Toolkit Script Files"
            Filters="*.wsc">
        </Folder>
        <Folder
            Name="Other Files"
            Filters="">
            <F N="../src/Applications/clsdeps.mak"/>
            <F N="../src/Applications/makefile.mak"/>
            <F N="../src/Applications/makeit.bat"/>
            <F N="../src/Applications/makeit_unix"/>
        </Folder>
    </Files>
    <Macro>
        <ExecMacro CmdLine="rem "/>
        <ExecMacro CmdLine="rem "/>
        <ExecMacro CmdLine="rem *************************************************************"/>
        <ExecMacro CmdLine="rem Set this to the base folder definition"/>
        <ExecMacro CmdLine='rem *************************************************************"'/>
