rm -f $OBJ/AttachmentCollection.dbo 
rm -f $OBJ/MailAddress.dbo 
rm -f $OBJ/MailAddressCollection.dbo 
rm -f $OBJ/MailMessage.dbo 
rm -f $OBJ/SmtpClient.dbo 
rm -f $OBJ/FtpClient.dbo 
rm -f $SYNEXPDIR/$NAMESPACE-contenttype.dbh 
rm -f $SYNEXPDIR/$NAMESPACE-contentdisposition.dbh 
rm -f $SYNEXPDIR/$NAMESPACE-ipendpoint.dbh 
rm -f $SYNEXPDIR/$NAMESPACE-socket.dbh 
rm -f $SYNEXPDIR/$NAMESPACE-networkstream.dbh 
rm -f $SYNEXPDIR/$NAMESPACE-tcpclient.dbh 
rm -f $SYNEXPDIR/$NAMESPACE-attachmentbase.dbh 
rm -f $SYNEXPDIR/$NAMESPACE-attachment.dbh 
rm -f $SYNEXPDIR/$NAMESPACE-attachmentcollection.dbh 
rm -f $SYNEXPDIR/$NAMESPACE-mailaddress.dbh 
rm -f $SYNEXPDIR/$NAMESPACE-mailaddresscollection.dbh 
rm -f $SYNEXPDIR/$NAMESPACE-mailmessage.dbh 
rm -f $SYNEXPDIR/$NAMESPACE-smtpclient.dbh 
rm -f $SYNEXPDIR/$NAMESPACE-ftpclient.dbh 
if [ "$NAMESPACE" != "*" ] 
then 
  rm -f $SYNEXPDIR/$NAMESPACE-*.dbh 
fi 
dblproto $VARIANT_FLAGS SRC:ContentType.dbc 
dbl $COMPILEFLAGS -TX -WD=919 $VARIANT_FLAGS  -o OBJ:ContentType.dbo SRC:ContentType.dbc 
dblproto $VARIANT_FLAGS SRC:ContentDisposition.dbc 
